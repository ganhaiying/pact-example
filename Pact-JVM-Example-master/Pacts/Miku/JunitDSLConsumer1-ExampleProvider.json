{
    "provider": {
        "name": "ExampleProvider"
    },
    "consumer": {
        "name": "JunitDSLConsumer1"
    },
    "interactions": [
        {
            "description": "Query name is Miku",
            "request": {
                "method": "GET",
                "path": "/information",
                "query": "name=Miku"
            },
            "response": {
                "status": 200,
                "headers": {
                    "Content-Type": "application/json;charset=UTF-8"
                },
                "body": {
                    "salary": 45000,
                    "name": "Hatsune Miku",
                    "nationality": "Japan",
                    "contact": {
                        "Email": "hatsune.miku@ariman.com",
                        "Phone Number": "9090950"
                    }
                }
            },
            "providerState": ""
        }
    ],
    "metadata": {
        "pact-specification": {
            "version": "2.0.0"
        },
        "pact-jvm": {
            "version": "3.5.12"
        }
    }
}